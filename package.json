{
  "name": "haxrugby",
  "description": "The HaxRugby hostbot. Based on HaxBall Headless API and Inversihax.",
  "version": "0.6.2",
  "author": "Dan Maia (aka JP)",
  "scripts": {
    "test": "mocha --require ts-node/register src/**/*.test.ts",
    "build": "browserify src/Bot/Main.ts -p [tsify] > dist/bundle.js",
    "build-uglify": "browserify src/Bot/Main.ts -p [tsify] | uglifyjs > dist/bundle_ugly.js",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint-fix": "eslint -c .eslintrc.js --ext .ts src --fix"
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "inversify": "^5.0.1",
    "inversihax": "^1.1.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/moment": "^2.13.0",
    "@types/node": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^6.0.2",
    "prettier": "^2.4.1",
    "ts-node": "^8.0.3",
    "tsify": "^4.0.1",
    "typemoq": "^2.1.0",
    "typescript": "^3.4.1",
    "uglify-es": "^3.3.9"
  }
}
